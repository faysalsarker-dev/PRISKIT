
{
  "compilerOptions": {
    "rootDir": "src",            // or "./src"
    "outDir": "dist",            // or "./dist"
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2023",
    "strict": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist", "src/generated"]
}
